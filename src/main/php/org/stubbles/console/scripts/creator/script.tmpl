#!/usr/bin/php
<?php
/**
 * Script to execute {NAMESPACE}\{CLASS}.
 *
 * @package  {NAMESPACE}
 */
namespace {NAMESPACE};
if (\Phar::running() !== '') {
    $rootDir     = \Phar::running();
    $projectPath = getcwd();
} elseif (file_exists(__DIR__ . '/../vendor/autoload.php')) {
    $rootDir     = __DIR__ . '/../';
    $projectPath = $rootDir;
} else {
    $rootDir     = __DIR__ . '/../../../../';
    $projectPath = $rootDir;
}

require $rootDir . '/vendor/autoload.php';
exit({CLASS}::main(realpath($projectPath), \net\stubbles\console\ConsoleOutputStream::forError()));
?>