#!/usr/bin/php
<?php
/**
 * Script to execute org\stubbles\console\scripts\creator\ConsoleAppCreator.
 *
 * @package  net\stubbles\console
 */
namespace org\stubbles\console\scripts\creator;
if (file_exists(__DIR__ . '/../vendor/.composer/autoload.php')) {
    $rootDir = __DIR__ . '/../';
} else {
    $rootDir = __DIR__ . '/../../../../';
}

require $rootDir . '/vendor/.composer/autoload.php';
exit(ConsoleAppCreator::main($rootDir, \net\stubbles\console\ConsoleOutputStream::forError()));
?>