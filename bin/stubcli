#!/usr/bin/php
<?php
/**
 * Script to execute an implementation of net\stubbles\console\ConsoleApp.
 *
 * @package  net\stubbles\console
 */
namespace net\stubbles\console;
if (file_exists(__DIR__ . '/../vendor/.composer/autoload.php')) {
    $rootDir = __DIR__ . '/../';
} else {
    $rootDir = __DIR__ . '/../../../../';
}

require $rootDir . '/vendor/.composer/autoload.php';
exit(ConsoleApp::stubcli($rootDir, $argv, ConsoleOutputStream::forError()));
?>