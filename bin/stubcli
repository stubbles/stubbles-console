#!/usr/bin/php
<?php
/**
 * Script to execute an implementation of net\stubbles\console\ConsoleApp.
 *
 * @package  net\stubbles\console
 */
namespace net\stubbles\console;
if (file_exists(__DIR__ . '/../bootstrap.php')) {
    require __DIR__ . '/../bootstrap.php';
} else {
    require __DIR__ . '/../../../../bootstrap.php';
}

exit(ConsoleApp::stubcli(\net\stubbles\Bootstrap::getRootPath(), $argv, ConsoleOutputStream::forError()));
?>