{
    "name": "stubbles/console",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Stubbles package to build command line apps.",
    "require": {
        "php": "^7.0",
        "stubbles/input": "^7.0",
        "stubbles/ioc": "^8.0.1",
        "stubbles/reflect": "^8.0",
        "stubbles/streams": "^8.0",
        "stubbles/values": "^8.1"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "^5.4",
        "bovigo/callmap": "^3.1",
        "bovigo/assert": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\console\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "org\\stubbles\\console\\test\\": "src/test/helper"
        }
    },
    "bin": ["bin/createConsoleApp"],
    "extra": {
        "branch-alias": {
            "dev-master": "7.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
