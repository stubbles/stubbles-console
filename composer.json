{
    "name": "stubbles/console",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Stubbles package to build command line apps.",
    "require": {
        "php": ">=5.3.0",
        "stubbles/core": "~3.0",
        "stubbles/input": "~2.3",
        "symfony/finder": "~2.2"
    },
    "require-dev": {
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": {
            "net\\stubbles\\console": "src/main/php",
            "org\\stubbles\\console": "src/main/php"
        }
    },
    "bin": ["bin/stubcli", "bin/createConsoleApp"],
    "extra": {
        "branch-alias": {
            "dev-master": "2.3.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
