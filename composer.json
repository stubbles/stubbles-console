{
    "name": "stubbles/console",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Stubbles package to build command line apps.",
    "require": {
        "php": ">=5.4.0",
        "stubbles/core": "^6.0",
        "stubbles/input": "^5.0"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.5",
        "phpunit/phpunit": "^4.6",
        "bovigo/callmap": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\console\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "org\\stubbles\\console\\test\\": "src/test/helper"
        },
        "files": [
            "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
        ]
    },
    "bin": ["bin/stubcli", "bin/createConsoleApp"],
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
