{
    "name": "stubbles/console",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Stubbles package to build command line apps.",
    "require": {
        "php": ">=5.6.0",
        "stubbles/input": "^6.0",
        "stubbles/ioc": "^7.1",
        "stubbles/reflect": "^7.0",
        "stubbles/streams": "^7.0",
        "stubbles/values": "^7.2"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "^5.4",
        "bovigo/callmap": "^2.0",
        "bovigo/assert": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\console\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "org\\stubbles\\console\\test\\": "src/test/helper"
        }
    },
    "bin": ["bin/stubcli", "bin/createConsoleApp"],
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
